<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/activity_main">

    <TextView
        android:id="@+id/teste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensor" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewSensor01"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/teste"
        android:layout_toEndOf="@+id/teste"
        android:layout_alignBottom="@+id/teste" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewResultadoSensor01"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textViewSensor01"
        android:layout_toEndOf="@+id/textViewSensor01"
        android:layout_marginLeft="57dp"
        android:layout_marginStart="57dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensor"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/teste"
        android:layout_toLeftOf="@+id/textViewSensor01"
        android:layout_toStartOf="@+id/textViewSensor01" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewResultadoSensor02"
        android:layout_alignTop="@+id/textViewSensor02"
        android:layout_alignLeft="@+id/textViewResultadoSensor01"
        android:layout_alignStart="@+id/textViewResultadoSensor01" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewSensor02"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignLeft="@+id/textViewSensor01"
        android:layout_alignStart="@+id/textViewSensor01" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensor"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewSensor03"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignLeft="@+id/textViewSensor02"
        android:layout_alignStart="@+id/textViewSensor02" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewResultadoSensor03"
        android:layout_alignBaseline="@+id/textViewSensor03"
        android:layout_alignBottom="@+id/textViewSensor03"
        android:layout_alignLeft="@+id/textViewResultadoSensor02"
        android:layout_alignStart="@+id/textViewResultadoSensor02" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensor"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewSensor04"
        android:layout_alignTop="@+id/textView9"
        android:layout_alignLeft="@+id/textViewSensor03"
        android:layout_alignStart="@+id/textViewSensor03" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewResultadoSensor04"
        android:layout_alignBaseline="@+id/textViewSensor04"
        android:layout_alignBottom="@+id/textViewSensor04"
        android:layout_alignLeft="@+id/textViewResultadoSensor03"
        android:layout_alignStart="@+id/textViewResultadoSensor03" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sensor"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="32dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewSensor05"
        android:layout_alignTop="@+id/textView12"
        android:layout_alignLeft="@+id/textViewSensor04"
        android:layout_alignStart="@+id/textViewSensor04" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewResultadoSensor05"
        android:layout_alignTop="@+id/textViewSensor05"
        android:layout_alignLeft="@+id/textViewResultadoSensor04"
        android:layout_alignStart="@+id/textViewResultadoSensor04" />


</RelativeLayout>
